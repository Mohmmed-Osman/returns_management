<odoo>
    <data noupdate="1">
        <record id="seq_return_sales_order" model="ir.sequence">
            <field name="name">Return Sale Order Reference</field>
            <field name="code">return.sale.order</field>
            <field name="prefix">RET/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_return_purchase_order" model="ir.sequence">
            <field name="name">Return Purchase Order Reference</field>
            <field name="code">return.purchase.order</field>
            <field name="prefix">REF/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>